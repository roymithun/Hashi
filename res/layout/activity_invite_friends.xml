<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <include
        android:id="@+id/navigation_bar"
        layout="@layout/navigation_bar" />

    <TextView
        android:id="@+id/txt_view_note"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/padding_bottom_15"
        android:paddingLeft="@dimen/padding_left_30"
        android:paddingRight="@dimen/padding_right_30"
        android:paddingTop="@dimen/padding_top_15"
        android:text="@string/strv_pick_frnds_note"
        android:textSize="12sp" />

    <ListView
        android:id="@+id/list_view_contacts"
        android:layout_width="match_parent"
        android:layout_height="match_parent" >
    </ListView>

</LinearLayout>